chan D0_0 = [1] of {int};
chan D1_0 = [1] of {int};
chan D1_1 = [1] of {int};
chan D2_0 = [1] of {int};
chan D4_0 = [1] of {int};

/* ========================================================= */ 
proctype P0() {
	atomic {
		if
		:: D1_0?1
		:: D2_0?1
		fi
	};
	D0_0!1;
	atomic {
		if
		:: D1_0?1
		:: D2_0?1
		fi
	}
}

/* ========================================================= */ 
proctype P1() {
	D1_0!1;
	D1_1!1
}

/* ========================================================= */ 
proctype P2() {
	atomic {
		if
		:: D4_0?1
		fi
	};
	D2_0!1
}

/* ========================================================= */ 
proctype P3() {
	atomic {
		D1_1?1
		
	};
	atomic {
		if
		:: D0_0?1
		fi
	}
}

/* ========================================================= */ 
proctype P4() {
	D4_0!1
}

/* ========================================================= */ 
init {
	run P0(); run P1(); run P2(); run P3(); run P4()
}